<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:wz="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context="com.wz.mobilesafetest2.activity.SettingCenterActivity">

    <TextView
        android:background="@drawable/blue_bkg"
        android:padding="10dp"
        android:gravity="center_horizontal"
        android:text="设置中心"
        android:textSize="22sp"
        android:layout_width="match_parent"
        android:layout_height="50dp" />

    <com.wz.mobilesafetest2.view.SettingView
        android:id="@+id/st_settingcenter_autoupdate"
        wz:setting_title="自动更新"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.wz.mobilesafetest2.view.SettingView>

    <com.wz.mobilesafetest2.view.SettingView
        android:id="@+id/st_settingcenter_black"
        wz:setting_title="黑名单拦截"
        wz:itebackground="middle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.wz.mobilesafetest2.view.SettingView>

    <com.wz.mobilesafetest2.view.SettingView
        wz:setting_title="last"
        wz:itebackground="last"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.wz.mobilesafetest2.view.SettingView>


</LinearLayout>
